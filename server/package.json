    "main": "src/server.js",
    "scripts": {
      "start:dev": "concurrently \"cd client && npm run build\" \"cd src && npm run server\"",
      "start": "npm run build && cd src && node server.js",
      "server": "cd src && nodemon server.js --ignore client",
      "build": "cd client && npm run build",
      "install": "cd src && npm i && cd ../client && npm i",
      "client": "cd client && npm start"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
      "express": "^4.17.1",
      "if-env": "^1.0.4"
    },
    "devDependencies": {
      "concurrently": "^5.2.0",
      "nodemon": "^2.0.4",
      "webpack": "^5.0.0",
      "webpack-cli": "^4.0.0"
    }
  }
  